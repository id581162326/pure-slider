{
  "name": "range-slider",
  "version": "1.0.0",
  "description": "Metalamp's educational program. The 4th task.",
  "main": "main.js",
  "scripts": {
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:demo": "better-npm-run build:demo",
    "postbuild:demo": "gh-pages -d demo",
    "build:test": "better-npm-run build:test"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack serve --config ./webpack/dev.webpack.config.ts",
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_FILES": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "THREADED": false
      }
    },
    "build:demo": {
      "command": "webpack --config ./webpack/demo.webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "TS_NODE_FILES": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "THREADED": false
      }
    },
    "build:prod": {
      "command": "webpack --config ./webpack/prod.webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "TS_NODE_FILES": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "THREADED": false
      }
    },
    "build:test": {
      "command": "karma start ./karma.config.ts",
      "env": {
        "NODE_ENV": "testing",
        "TS_NODE_FILES": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "THREADED": false
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/id581162326/range-slider.git"
  },
  "author": "Pavel Shapov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/id581162326/range-slider/issues"
  },
  "homepage": "https://github.com/id581162326/range-slider#readme",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.17",
    "@types/css-minimizer-webpack-plugin": "^3.0.0",
    "@types/jasmine": "^3.7.6",
    "@types/karma": "^6.3.0",
    "@types/karma-webpack": "^2.0.9",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "babel-loader": "^8.2.2",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "gh-pages": "^3.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.2.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-typescript": "^5.5.1",
    "karma-webpack": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.8",
    "mocha": "^8.4.0",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^5.3.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "fp-ts": "^2.10.5"
  }
}
